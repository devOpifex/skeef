  },
  "homepage": "https://github.com/devOpifex/skeef#readme",
  "dependencies": {
    "esbuild": "^0.8.56"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "cssnano": "^4.1.10",
    "postcss": "^8.2.7",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.3"
  }
